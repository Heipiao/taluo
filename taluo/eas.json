{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "image": "latest"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium",
        "image": "latest",
        "credentialsSource": "remote",
        "autoIncrement": true,
        "buildConfiguration": "Release",
        "simulator": true 
      }
    }
  }
}
